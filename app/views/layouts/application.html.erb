<!DOCTYPE html>
<html>
<head>
  <title>G2MidtermAssessment</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<header>
  <div class="container">
    <div class="logo">
      <%= link_to "gBook", root_path %>
    </div>
    <% if current_user %>
      <div class="logout">
        <%= link_to "Logout", signout_path %>
      </div>
    <% end %>
  </div>
</header>
<main>
  <div class="container">
    <%= yield %>
  </div>
</main>
<footer>
  <div class="container"></div>
</footer>
</body>
</html>
